@model List<SistemaGCS.Models.Miembro_Elemento>
@{
    ViewBag.Title = "Index";

    List<SistemaGCS.Models.Miembro_Elemento> TipoU = ViewBag.listar;
}

<!-- Page header -->
<dv class="full-box page-header">
    <h3 class="text-left">

        @*<a href="~/Miembro_Elemento/Agregar" class="btn btn-success">Registrar Nuevo </a>*@
        <a href="##" class="btn btn-primary" onClick="history.go(-1); return false;">Volver</a>
    </h3>

</dv>
<!--CONTENT-->
<!-- Content here-->
<div class="container-fluid">
    @using (Html.BeginForm("IndexListar", "Miembro_Elemento", FormMethod.Post))
    {
    }
    <div class="table-responsive">
        <table id="datatable" class="display" style="width:100%">
            <thead>
                <tr class="text-center">

                    <th>Id</th>
                    <th>Miembro </th>
                    <th>Elemento </th>
                    <th>Cronograma </th>
                    <th>Acciones</th>

                </tr>
            </thead>

            <tbody>
                @foreach (var item in TipoU)
                {
                    <tr class="text-center">
                        <td>@(item.Id_miembroelemento)</td>
                        <td>@(item.Miembro_Proyecto.Usuario.Apellido), @(item.Miembro_Proyecto.Usuario.Nombre)   </td>
                        <td>@(item.Elemento_Configuracion.Nombre)</td>
                        <td>@(item.FechaInicio) - @(item.Fechafin) </td>
                        <td>
                            @*<a onclick="return confirm('Esta Seguro que desea Actualizar Datos del registro?')" href="~/Miembro_Elemento/Agregar/@item.Id_miembroelemento" class="btn btn-primary">Editar</a>*@
                            
                            <a href="@item.Url" class="btn btn-outline-info">Ver Documento</a>
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>

